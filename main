 * write any Karel program you want.
 **/
function main(){
   beepersRight();
   goUpTurnLeft();
   beepersLeft();
   goUpTurnRight();
      beepersRight();
   goUpTurnLeft();
   beepersLeft();
   goUpTurnRight();
      beepersRight();
   
   turnRight();
   
   beepersLeft();
   goUpTurnRight();
      beepersRight();
   goUpTurnLeft();
   beepersLeft();
   goUpTurnRight();
      beepersRight();
   goUpTurnLeft();
   beepersLeft();
   
   turnLeft();
   //Finished placing beepers
   
   pickBeepersRight();
   goUpTurnLeft();
   pickBeepersLeft();
   goUpTurnRight();
      pickBeepersRight();
   goUpTurnLeft();
   pickBeepersLeft();
   goUpTurnRight();
      pickBeepersRight();
   
   turnRight();
   
   pickBeepersLeft();
   goUpTurnRight();
      pickBeepersRight();
   goUpTurnLeft();
   pickBeepersLeft();
   goUpTurnRight();
      pickBeepersRight();
   goUpTurnLeft();
   pickBeepersLeft();
   
   turnLeft();
   
}
 
function goUpTurnRight() {
   turnRight();
   move();
   turnRight();
}
 
function goUpTurnLeft() {
   turnLeft();
   move();
   turnLeft();
}
 
function beepersRight() {
   putBeeper();
   move();
   move();
   putBeeper();
   move();
   move();
   putBeeper();  
}
 
function beepersLeft() {
   move();
   putBeeper();
   move();
   move();
   putBeeper();
   move();
}

function pickBeepersRight() {
   pickBeeper();
   move();
   move();
   pickBeeper();
   move();
   move();
   pickBeeper();  
}
 
function pickBeepersLeft() {
   move();
   pickBeeper();
   move();
   move();
   pickBeeper();
   move();
}
